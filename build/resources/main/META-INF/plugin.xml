<idea-plugin>
  <version>1.0-SNAPSHOT</version>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="201.7223" until-build="201.*"/>
  <id>de.local-fwx.intellij-simple-link-bar</id>
  <name>StackOverflow</name>
  <vendor email="schult.stephan@gmail.com" url="https://github.com/stephnan">Stephnan</vendor>
  <description>
    Adding some custom links to Intellijs Menu Bar
    </description>
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <action id="StackOverflow.showLinksAction.MainMenu" class="de.localfwx.intellijplugins.linkbar.showLinksAction" text="Ask Question on Stack Overflow" description="Ask a Question on Stack Overflow">
      <add-to-group group-id="ToolsMenu" anchor="last"/>
    </action>
  </actions>
</idea-plugin>
